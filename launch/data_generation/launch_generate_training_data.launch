<launch>
	<arg name="num_positive_data" default="1"/>
	<arg name="yaml_filename" default="right_hand_left_stance"/>
    <arg name="visualize" default="true" />  
    <arg name="positive_data_only" default="true" />

    <node name="generate_data_$(arg yaml_filename)" pkg="avatar_locomanipulation" type="test_generate_data" args="$(arg num_positive_data) $(arg yaml_filename).yaml $(arg visualize) $(arg positive_data_only)" output="screen" /> 
</launch>